
upstream payment_bracnet {
    server 202.168.232.8:8000;
}

server {

    listen 80;

    location / {
        proxy_pass http://hello_django;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header Host $host;
        proxy_redirect off;
    }

    location /static/ {
        alias /bracnet_payment_api/static;
    }

}
    
    

    
        
        

        
